{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2017",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@containers/*": ["src/containers/*"],
      "@fonts/*": ["src/assets/fonts/*"],
      "@images/*": ["src/assets/images/*"],
      "@lib/*": ["src/lib/*"],
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/assets/styles/*"]
    }
  },
  "files": [
    "src/**/*"
  ]
}
